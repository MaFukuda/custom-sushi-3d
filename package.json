{
  "name": "grimoirejs-register-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "grimoirejs": "^0.12.1",
    "grimoirejs-math": "^1.7.0",
    "grimoirejs-torus": "git://github.com/GrimoireGL/grimoirejs-torus.git#master",
    "regenerator-runtime": "^0.10.0",
    "shifty": "^1.5.4",
    "vue": "2.1.6",
    "vue-color": "^2.0.3"
  },
  "scripts": {
    "watch": "npm-run-all --parallel css-watch js-watch",
    "build": "npm-run-all --parallel css-build js-build",
    "js-build": "browserify -v -d index.babel.js | exorcist index.js.map -b ./ > index.js",
    "js-watch": "watchify -v -d index.babel.js -o 'exorcist index.js.map > index.js'",
    "css-watch": "stylus index.styl -w --include-css -r -m -u poststylus --with \"[ 'autoprefixer' ]\" -m --sourcemap-base ./ -o index.css",
    "css-build": "stylus index.styl --include-css -r -m -u poststylus --with \"[ 'autoprefixer' ]\" -m --sourcemap-base ./ -o index.css"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.1",
    "exorcist": "^0.4.0",
    "grimoirejs-fundamental": "^0.9.0",
    "npm-run-all": "^4.0.0",
    "poststylus": "^0.2.3",
    "stylus": "^0.54.5",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "brfs"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common"
  }
}
